# Be Bourne compatible
if test -n "${ZSH_VERSION+set}" && (emulate sh) >/dev/null 2>&1; then
  emulate sh
  NULLCMD=:
  # Zsh 3.x and 4.x performs word splitting on ${1+"$@"}, which
  # is contrary to our usage.  Disable this feature.
  alias -g '${1+"$@"}'='"$@"'
fi

perllibdir="/root/project/openwinrt/automake/automake-1.12/lib:/root/project/openwinrt/automake/automake-1.12/lib"
export perllibdir
exec "/root/project/openwinrt/automake/automake-1.12/automake" "--libdir=/root/project/openwinrt/automake/automake-1.12/lib" ${1+"$@"}
